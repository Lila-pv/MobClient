<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
    <inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
    <inherits name='com.badlogic.gdx.physics.box2d.box2d-gwt' />
    <inherits name='com.artemis.backends.artemis_backends_gwt' />
    <inherits name='Box2DLights' />
    <inherits name='com.badlogic.gdx.ai' />

    <extend-configuration-property name="artemis.reflect.include" value="com.mob.client.artemis.components" />
    <extend-configuration-property name="artemis.reflect.include" value="com.mob.client.artemis.systems" />
    <extend-configuration-property name="artemis.reflect.include" value="com.mob.client.screens" />
    <extend-configuration-property name="gdx.reflect.include" value="com.mob.client.dao.loaders" />
    <extend-configuration-property name="gdx.reflect.include" value="com.mob.client.dao.objects" />
    <extend-configuration-property name="gdx.reflect.include" value="com.mob.client.dao.readers" />

    <inherits name='MobClient' />
    <entry-point class='com.mob.client.client.HtmlLauncher' />

    <set-configuration-property name="gdx.assetpath" value="../android/assets" />
</module>
